<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.emotilog.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_summary"
        android:name="com.example.emotilog.ui.summary.SummaryFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_summary" />

    <fragment
        android:id="@+id/navigation_logs"
        android:name="com.example.emotilog.ui.Logs.LogsFragment"
        android:label="Logs"
        tools:layout="@layout/fragment_logs" />
</navigation>